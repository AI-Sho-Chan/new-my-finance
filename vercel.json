{
    "builds":  [
                   {
                       "src":  "web/package.json",
                       "use":  "@vercel/static-build",
                       "config":  {
                                      "distDir":  "dist"
                                  }
                   },
                   {
                       "src":  "api/**/*.mjs",
                       "use":  "@vercel/node"
                   }
               ],
    "routes":  [
                   {
                       "src":  "/api/(.*)",
                       "dest":  "/api/$1"
                   },
                   {
                       "src":  "/react",
                       "dest":  "/index.html"
                   },
                   {
                       "src":  "/assets/(.*)",
                       "dest":  "/assets/$1"
                   },
                   {
                       "src":  "/",
                       "dest":  "/NMY.html"
                   }
               ]
}
